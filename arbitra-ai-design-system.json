  "gradients": {
    "sunset": {
      "name": "Main Sunset Gradient",
      "direction": "135deg",
      "stops": [
        {
          "position": "0%",
          "color": "hsl(28 100% 55%)",
          "hex": "#FF8C1A",
          "rgb": "rgb(255, 140, 26)"
        },
        {
          "position": "50%",
          "color": "hsl(35 100% 60%)",
          "hex": "#FFAD33",
          "rgb": "rgb(255, 173, 51)"
        },
        {
          "position": "100%",
          "color": "hsl(45 100% 65%)",
          "hex": "#FFD633",
          "rgb": "rgb(255, 214, 51)"
        }
      ],
      "css": "linear-gradient(135deg, hsl(28 100% 55%) 0%, hsl(35 100% 60%) 50%, hsl(45 100% 65%) 100%)"
    },
    "vertical": {
      "name": "Vertical Gradient for Buttons",
      "direction": "180deg",
      "stops": [
        {
          "position": "0%",
          "color": "hsl(28 100% 55%)",
          "hex": "#FF8C1A",
          "rgb": "rgb(255, 140, 26)"
        },
        {
          "position": "100%",
          "color": "hsl(40 100% 55%)",
          "hex": "#FFB31A",
          "rgb": "rgb(255, 179, 26)"
        }
      ],
      "css": "linear-gradient(180deg, hsl(28 100% 55%) 0%, hsl(40 100% 55%) 100%)"
    },
    "lightBg": {
      "name": "Subtle Background Gradient",
      "direction": "135deg",
      "stops": [
        {
          "position": "0%",
          "color": "hsl(40 100% 98%)",
          "hex": "#FFFEF9",
          "rgb": "rgb(255, 254, 249)"
        },
        {
          "position": "50%",
          "color": "hsl(35 50% 95%)",
          "hex": "#F8F3E8",
          "rgb": "rgb(248, 243, 232)"
        },
        {
          "position": "100%",
          "color": "hsl(40 100% 98%)",
          "hex": "#FFFEF9",
          "rgb": "rgb(255, 254, 249)"
        }
      ],
      "css": "linear-gradient(135deg, hsl(40 100% 98%) 0%, hsl(35 50% 95%) 50%, hsl(40 100% 98%) 100%)"
    }
  },
  "shadows": {
    "glowOrange": {
      "name": "Orange Glow",
      "css": "0 0 60px hsl(28 100% 55% / 0.3)",
      "rgb": "0 0 60px rgba(255, 140, 26, 0.3)"
    },
    "button": {
      "name": "Button Shadow",
      "css": "0 4px 20px hsl(28 100% 55% / 0.4)",
      "rgb": "0 4px 20px rgba(255, 140, 26, 0.4)"
    },
    "card": {
      "name": "Card Shadow (Light Mode)",
      "css": "0 4px 30px hsl(28 100% 55% / 0.1)",
      "rgb": "0 4px 30px rgba(255, 140, 26, 0.1)"
    }
  },
  "effects": {
    "glassCard": {
      "name": "Glass Card (Light Mode)",
      "background": {
        "hsl": "hsl(40 50% 98% / 0.9)",
        "rgb": "rgba(255, 254, 249, 0.9)"
      },
      "backdropFilter": "blur(20px)",
      "border": {
        "default": {
          "hsl": "1px solid hsl(28 100% 55% / 0.15)",
          "rgb": "1px solid rgba(255, 140, 26, 0.15)"
        },
        "hover": {
          "hsl": "1px solid hsl(28 100% 55% / 0.3)",
          "rgb": "1px solid rgba(255, 140, 26, 0.3)"
        }
      },
      "hover": {
        "background": {
          "hsl": "hsl(40 50% 100% / 0.95)",
          "rgb": "rgba(255, 255, 255, 0.95)"
        },
        "shadow": {
          "hsl": "0 0 30px hsl(28 100% 55% / 0.15)",
          "rgb": "0 0 30px rgba(255, 140, 26, 0.15)"
        }
      }
    }
  },
  "animations": {
    "pulseGlow": {
      "name": "Pulse Glow Animation",
      "duration": "2s",
      "timing": "ease-in-out",
      "iteration": "infinite",
      "keyframes": {
        "0%, 100%": {
          "shadow": {
            "hsl": "0 0 20px hsl(28 100% 55% / 0.2)",
            "rgb": "0 0 20px rgba(255, 140, 26, 0.2)"
          }
        },
        "50%": {
          "shadow": {
            "hsl": "0 0 40px hsl(28 100% 55% / 0.4)",
            "rgb": "0 0 40px rgba(255, 140, 26, 0.4)"
          }
        }
      }
    }
  },
  "cssVariables": {
    "primary": "--primary: 28 100% 55%;",
    "primaryForeground": "--primary-foreground: 20 14% 4%;",
    "secondary": "--secondary: 40 100% 65%;",
    "secondaryForeground": "--secondary-foreground: 20 14% 4%;",
    "accent": "--accent: 35 100% 60%;",
    "accentForeground": "--accent-foreground: 20 14% 4%;",
    "sunsetOrange": "--sunset-orange: 28 100% 55%;",
    "sunsetAmber": "--sunset-amber: 35 100% 60%;",
    "sunsetYellow": "--sunset-yellow: 45 100% 65%;",
    "sunsetGold": "--sunset-gold: 40 100% 50%;",
    "background": "--background: 40 100% 97%;",
    "foreground": "--foreground: 20 14% 15%;",
    "card": "--card: 40 50% 98%;",
    "cardForeground": "--card-foreground: 20 14% 15%;",
    "muted": "--muted: 40 20% 92%;",
    "mutedForeground": "--muted-foreground: 20 10% 45%;",
    "border": "--border: 35 30% 85%;",
    "input": "--input: 35 30% 90%;",
    "ring": "--ring: 28 100% 55%;"
  },
  "designPrinciples": [
    "Use cream/white backgrounds with warm tint",
    "Orange as primary action color",
    "Gradient buttons and accents",
    "Subtle orange glows on hover",
    "Warm borders (not pure gray)",
    "RTL support for Hebrew"
  ]
}




